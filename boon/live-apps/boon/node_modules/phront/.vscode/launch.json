    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "attach",
            "name": "Attach to DataWorker",
            "port": 1235,
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch phront-service-usage",
            "runtimeVersion": "12.18.4",
            "program": "${workspaceFolder}/test/spec/phront-service-usage/index.js",
            "cwd": "${workspaceFolder}/test/spec/phront-service-usage/",
            "trace": false,
            "enableContentValidation": false,
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch test-google-calendar-service",
            "program": "${workspaceFolder}/data/google-calendar.datareel/service/test-google-calendar-service/index.js",
            "cwd": "${workspaceFolder}/data/google-calendar.datareel/service/test-google-calendar-service",
            "autoAttachChildProcesses": true,
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Google Calendar API Sandbox",
            "program": "${workspaceFolder}/data/google-calendar.datareel/service/api-sandbox/index.js",
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch test-JSON-parse-reviver",
            "program": "${workspaceFolder}/sandbox/test-JSON-parse-reviver.js",
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch via NPM",
            "runtimeExecutable": "npm",
            "cwd": "${workspaceFolder}",
            "runtimeArgs": [
                "run-script",
                "test"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceFolder}/services/shopify-admin-scripts/get-customers.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "npm test",
            "autoAttachChildProcesses": true,
            "runtimeExecutable": "~/.nvm/versions/node/v12.16.1/bin/node",
            "cwd": "${workspaceFolder}",
            "program": "${workspaceFolder}/test/run-node.js",
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "npm test 2",
            "runtimeExecutable": "/Users/benoit/.nvm/versions/node/v10.16.3/bin/npm",
            "runtimeArgs": [
                "run-script",
                "test",
                "--inspect-brk=27190"
            ],
            "port": 27190
        },
        {
            "name": "debug npm test",
            "type": "node",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "/Users/benoit/.nvm/versions/node/v10.16.3/bin/node",
            "runtimeArgs": [
                "test/run-node.js"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Spec",
            "program": "${workspaceFolder}/test/run-node.js",
            "skipFiles": [
                "<node_internals>/**"
            ]
        }
    ]
}
