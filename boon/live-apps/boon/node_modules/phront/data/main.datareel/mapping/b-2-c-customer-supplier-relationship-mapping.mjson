        "values": {
            "convertExpression": "b2cCustomerSupplierRelationshipId == $"
        }
    },
    "contactForms-value-converter": {
        "prototype": "montage/data/converter/raw-foreign-value-to-object-converter",
        "values": {
            "convertExpression": "b2cCustomerSupplierRelationshipId == $"
        }
    },
