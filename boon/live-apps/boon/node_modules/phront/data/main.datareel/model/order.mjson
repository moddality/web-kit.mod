{
    "root": {
        "prototype": "montage/core/meta/module-object-descriptor",
        "values": {
            "name": "Order",
            "propertyDescriptors": [
                {"@": "name"},
                {"@": "customer"},
                {"@": "orderLineItems"},
                {"@": "transactions"},
                {"@": "billingAddress"},
                {"@": "shippingAddress"},
                {"@": "doesBillingAddressMatchShippingAddress"},
                {"@": "cancellationDate"},
                {"@": "cancellationReason"}
            ],
            "objectDescriptorModule": {
                "%": "./order.mjson"
            },
            "exportName": "Order",
            "module": {
                "%": "./order"
            },
            "object":{"@": "order"},
            "parent":{"@": "Object"}
        }
    },
    "order": {
        "object": "./order"
    },
    "Object": {
        "object": "./data-object.mjson"
    },
    "name": {
        "prototype": "montage/core/meta/property-descriptor",
        "values": {
            "name": "name",
            "valueType": "string"
        }
    },
    "customer": {
        "prototype": "montage/core/meta/property-descriptor",
        "values": {
            "name": "customer",
            "valueType": "object"
        }
    },
     "orderLineItems": {
        "prototype": "montage/core/meta/property-descriptor",
        "values": {
            "name": "orderLineItems",
            "cardinality": -1,
            "valueDescriptor": {"@": "order-line-item_descriptor"}
        }
    },
     "order-line-item_descriptor": {
        "object": "./order-line-item.mjson"
    },
    "transactions": {
        "prototype": "montage/core/meta/property-descriptor",
        "values": {
            "name": "transactions",
            "cardinality": -1,
            "inversePropertyName": "order",
            "valueDescriptor": {"@": "order-transaction"}
        }
    },
     "order-transaction": {
        "object": "./order-transaction.mjson"
    },

    "billingAddress": {
        "prototype": "montage/core/meta/property-descriptor",
        "values": {
            "name": "billingAddress",
            "cardinality": 1,
            "valueDescriptor": {"@": "address_descriptor"}
        }
    },
    "shippingAddress": {
        "prototype": "montage/core/meta/property-descriptor",
        "values": {
            "name": "shippingAddress",
            "cardinality": 1,
            "valueDescriptor": {"@": "address_descriptor"}
        }
    },
    "address_descriptor": {
        "object": "./messaging-channel/postal-address.mjson"
    },
    "doesBillingAddressMatchShippingAddress": {
        "prototype": "montage/core/meta/property-descriptor",
        "values": {
            "name": "doesBillingAddressMatchShippingAddress",
            "valueType": "boolean"
        }
    },
    "cancellationDate": {
        "prototype": "montage/core/meta/property-descriptor",
        "values": {
            "name": "cancellationDate",
            "valueType": "date"
        }
    },
    "cancellationReason": {
        "prototype": "montage/core/meta/property-descriptor",
        "values": {
            "name": "cancellationReason",
            "cardinality": 1,
            "valueType": "enum",
            "enumValues": [
                "Customer",
                "Declined",
                "Fraud",
                "Inventory",
                "Other"
            ]
        }
    }
