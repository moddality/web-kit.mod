{
    "root": {
        "prototype": "montage/core/meta/module-object-descriptor",
        "values": {
            "name": "ProductVariant",
            "propertyDescriptors": [
                {"@": "title"},
                {"@": "product"},
                {"@": "images"},
                {"@": "price"},
                {"@": "selectedOptions"},
                {"@": "sku"},
                {"@": "availableForSale"},
                {"@": "presentmentPrices"}
            ],
            "objectDescriptorModule": {
                "%": "./product-variant.mjson"
            },
            "exportName": "ProductVariant",
            "module": {
                "%": "./product-variant"
            },
            "object":{"@": "ProductVariant"},
            "parent":{"@": "objectDescriptor"}
        }
    },
    "ProductVariant": {
        "object": "./product-variant"
    },
    "objectDescriptor": {
        "object": "./data-object.mjson"
    },
    "title": {
        "prototype": "montage/core/meta/property-descriptor",
        "values": {
            "name": "title",
            "valueType": "string"
        }
    },
    "product": {
        "prototype": "montage/core/meta/property-descriptor",
        "values": {
            "name": "product",
            "cardinality": 1,
            "inversePropertyName": "variants",
            "valueDescriptor": {"@": "productDescriptor"}
        }
    },
     "images": {
        "prototype": "montage/core/meta/property-descriptor",
        "values": {
            "name": "images",
            "cardinality": -1,
            "valueDescriptor": {"@": "image_descriptor"}
        }
    },
    "price": {
        "prototype": "montage/core/meta/property-descriptor",
        "values": {
            "name": "price",
            "valueType": "number"
        }
    },
    "selectedOptions": {
        "prototype": "montage/core/meta/property-descriptor",
        "values": {
            "name": "selectedOptions",
            "valueType": "object",
            "cardinality": -1
        }
    },
    "sku": {
        "prototype": "montage/core/meta/property-descriptor",
        "values": {
            "name": "sku",
            "valueType": "string"
        }
    },
    "availableForSale": {
        "prototype": "montage/core/meta/property-descriptor",
        "values": {
            "name": "availableForSale",
            "valueType": "boolean"
        }
    },
    "presentmentPrices": {
        "prototype": "montage/core/meta/property-descriptor",
        "values": {
            "name": "presentmentPrices",
            "cardinality": -1,
            "valueType": "object"
        }
    },
    "productDescriptor": {
        "object": "./product.mjson"
    },
    "image_descriptor": {
        "object": "./image.mjson"
    }

}
